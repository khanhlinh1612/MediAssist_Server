<div class='mt-4'>
    <div class='row'>
        <h1>Home</h1>
        {{#each users}}
            <div class='col-sm-3 col-5'>
                <div class='card mb-2 ms-2' style='width: 18rem;'>
                    <img src={{this.avatar}} class='card-img-top' alt='...' />
                    <div class='card-body'>
                        <h5 class='card-title'>{{this.first_name}}</h5>
                        <p class='card-text'>{{this.last_name}}</p>
                        <a href='#' class='btn btn-primary'>Go somewhere</a>
                    </div>
                </div>
            </div>
        {{/each}}
    </div>
</div>
